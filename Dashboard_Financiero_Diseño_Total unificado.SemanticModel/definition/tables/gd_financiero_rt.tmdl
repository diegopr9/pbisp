	measure 'Prima Emitida' = ```
			
			VAR PeriodoSeleccionado = MAX(Calendario[Date])
			VAR AnioActual = YEAR(PeriodoSeleccionado)
			VAR MesActual  = MONTH(PeriodoSeleccionado)
			VAR MesActual_Inicio = DATE(AnioActual, MesActual, 1)
			
			-- Periodo anterior (12 meses atrás)
			VAR PeriodoAnterior = EDATE(MesActual_Inicio, -12)
			VAR AnioAnterior    = YEAR(PeriodoAnterior)
			VAR MesAnterior     = MONTH(PeriodoAnterior)
			--- === DEFINICIÓN DE FILTROS (Lógica de tiempo) ===
			VAR FiltroPeriodoMensualizado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        YEAR ( DATEVALUE( Calendario[MesNombreAño] ) ) = AnioActual
			            && MONTH ( DATEVALUE( Calendario[MesNombreAño] ) ) = MesActual
			    )
			
			VAR FiltroAcumulado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        DATEVALUE( Calendario[MesNombreAño] ) >= DATE( AnioActual, 1, 1 )
			            && DATEVALUE( Calendario[MesNombreAño] ) <= EOMONTH( MesActual_Inicio, 0 )
			    )
			
			VAR FiltroAnualizado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        DATEVALUE( Calendario[MesNombreAño] ) >  EDATE( MesActual_Inicio, -12 )   -- últimos 12 meses
			            && DATEVALUE( Calendario[MesNombreAño] ) <= EOMONTH( MesActual_Inicio, 0 )
			    )
			
			-----FILTROS ANTERIORES
			VAR FiltroPeriodoAnteriorMensualizado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        YEAR ( DATEVALUE( Calendario[MesNombreAño] ) ) = AnioAnterior
			            && MONTH ( DATEVALUE( Calendario[MesNombreAño] ) ) = MesAnterior
			    )
			
			VAR FiltroPeriodoAnteriorAcumulado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        DATEVALUE( Calendario[MesNombreAño] ) >= DATE( AnioAnterior, 1, 1 )
			            && DATEVALUE( Calendario[MesNombreAño] ) <= EOMONTH( PeriodoAnterior, 0 )
			    )
			
			VAR FiltroPeriodoAnteriorAnualizado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        DATEVALUE( Calendario[MesNombreAño] ) >  EDATE( PeriodoAnterior, -12 )
			            && DATEVALUE( Calendario[MesNombreAño] ) <= EOMONTH( PeriodoAnterior, 0 )
			    )
			--- === CAPTURA DE SELECCIONES ===
			VAR TipoValor   = SELECTEDVALUE(SelectorValor[TipoValor])
			VAR TipoCalculo = SELECTEDVALUE(SelectorCalculo_PG[TipoCalculo])
			
			--- === CÁLCULOS DINÁMICOS SEGÚN SELECCIÓN DE VALOR ===
			
			-- 1. Cálculo de REAL ACTUAL (Basado en SelectorValor)
			VAR REAL_ACT = 
			    SWITCH( TipoValor,
			        "MENSUALIZADO", 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"51"}, FiltroPeriodoMensualizado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"45"}, FiltroPeriodoMensualizado),
			        "ACUMULADO", 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"51"}, FiltroAcumulado) - 
			           CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"45"}, FiltroAcumulado),
			        "ANUALIZADO", 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"51"}, FiltroAnualizado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"45"}, FiltroAnualizado),
			        0
			    )
			
			-- 2. Cálculo de REAL AÑO ANTERIOR (Basado en SelectorValor)
			VAR REAL_Y_1 = 
			    SWITCH( TipoValor,
			        "MENSUALIZADO", 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"51"}, FiltroPeriodoAnteriorMensualizado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"45"}, FiltroPeriodoAnteriorMensualizado),
			        "ACUMULADO", 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"51"}, FiltroPeriodoAnteriorAcumulado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"45"}, FiltroPeriodoAnteriorAcumulado),
			        "ANUALIZADO", 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"51"}, FiltroPeriodoAnteriorAnualizado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n2] IN {"45"}, FiltroPeriodoAnteriorAnualizado),
			        0
			    )
			
			-- 3. Presupuesto (Asumiendo que sigue la lógica mensualizada o según tu definición)
			VAR Ppto = 
			    SWITCH( TipoValor,
			        "MENSUALIZADO", CALCULATE(SUM(gd_financiero_rt[val_presupuesto]), gd_financiero_rt[cta_des_n2] = "Prima Emitida", FiltroPeriodoMensualizado),
			        "ACUMULADO",    CALCULATE(SUM(gd_financiero_rt[val_presupuesto]), gd_financiero_rt[cta_des_n2] = "Prima Emitida", FiltroAcumulado),
			        "ANUALIZADO",   CALCULATE(SUM(gd_financiero_rt[val_presupuesto]), gd_financiero_rt[cta_des_n2] = "Prima Emitida", FiltroAnualizado),
			        0
			    )
			
			--- === RETORNO FINAL (Selector de Cálculo) ===
			RETURN
			    SWITCH (
			        TipoCalculo,
			        "Real Act", COALESCE(DIVIDE(REAL_ACT, 1000, 0), 0),
			        "Ppto",     COALESCE(DIVIDE(Ppto, 1000, 0), 0),
			        "% Cumpl.", COALESCE((DIVIDE(REAL_ACT, Ppto, 0) * 100), 0),
			        "Real Y-1", COALESCE(DIVIDE(REAL_Y_1, 1000, 0), 0),
			        "%YoY",     COALESCE((DIVIDE(REAL_ACT - REAL_Y_1, REAL_Y_1, 0) * 100), 0)
			    )
			```

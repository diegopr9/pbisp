	measure 'Reserva Técnica Neta' = ```
			
			VAR PeriodoSeleccionado = MAX(Calendario[Date])
			VAR AnioActual = YEAR(PeriodoSeleccionado)
			VAR MesActual  = MONTH(PeriodoSeleccionado)
			VAR MesActual_Inicio = DATE(AnioActual, MesActual, 1)
			
			-- Periodo anterior (12 meses atrás)
			VAR PeriodoAnterior = EDATE(MesActual_Inicio, -12)
			VAR AnioAnterior    = YEAR(PeriodoAnterior)
			VAR MesAnterior     = MONTH(PeriodoAnterior)
			--- === DEFINICIÓN DE FILTROS (Lógica de tiempo) ===
			VAR FiltroPeriodoMensualizado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        YEAR ( DATEVALUE( Calendario[MesNombreAño] ) ) = AnioActual
			            && MONTH ( DATEVALUE( Calendario[MesNombreAño] ) ) = MesActual
			    )
			
			VAR FiltroAcumulado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        DATEVALUE( Calendario[MesNombreAño] ) >= DATE( AnioActual, 1, 1 )
			            && DATEVALUE( Calendario[MesNombreAño] ) <= EOMONTH( MesActual_Inicio, 0 )
			    )
			
			VAR FiltroAnualizado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        DATEVALUE( Calendario[MesNombreAño] ) >  EDATE( MesActual_Inicio, -12 )   -- últimos 12 meses
			            && DATEVALUE( Calendario[MesNombreAño] ) <= EOMONTH( MesActual_Inicio, 0 )
			    )
			
			-----FILTROS ANTERIORES
			VAR FiltroPeriodoAnteriorMensualizado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        YEAR ( DATEVALUE( Calendario[MesNombreAño] ) ) = AnioAnterior
			            && MONTH ( DATEVALUE( Calendario[MesNombreAño] ) ) = MesAnterior
			    )
			
			VAR FiltroPeriodoAnteriorAcumulado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        DATEVALUE( Calendario[MesNombreAño] ) >= DATE( AnioAnterior, 1, 1 )
			            && DATEVALUE( Calendario[MesNombreAño] ) <= EOMONTH( PeriodoAnterior, 0 )
			    )
			
			VAR FiltroPeriodoAnteriorAnualizado =
			    FILTER (
			        ALL ( Calendario[MesNombreAño] ),
			        DATEVALUE( Calendario[MesNombreAño] ) >  EDATE( PeriodoAnterior, -12 )
			            && DATEVALUE( Calendario[MesNombreAño] ) <= EOMONTH( PeriodoAnterior, 0 )
			    )
			--- === CAPTURA DE SELECCIONES ===
			
			
			VAR TipoValor = SELECTEDVALUE(SelectorValor[TipoValor])
			VAR TipoData  = SELECTEDVALUE(SelectorData[TipoData])
			----------------------------------------------------------------------------------
			/* === Reserva Técnica Neta === */
			
			-----------------------------------------------------------
			
			-- REAL ACTUAL (Reserva Neta - Reserva G2)
			VAR REAL_ACT = 
			    SWITCH( TipoValor,
			        "MENSUALIZADO", 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"5701","5702"}, FiltroPeriodoMensualizado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"4801","4802"}, FiltroPeriodoMensualizado),
			        "ACUMULADO",    
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"5701","5702"}, FiltroAcumulado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"4801","4802"}, FiltroAcumulado),
			        "ANUALIZADO",   
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"5701","5702"}, FiltroAnualizado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"4801","4802"}, FiltroAnualizado),
			        0
			    )
			
			-- PRESUPUESTO (Reserva Ppto - Reserva G2 Ppto)
			VAR Ppto = 
			    SWITCH( TipoValor,
			        "MENSUALIZADO", 
			            CALCULATE(SUM(gd_financiero_rt[val_presupuesto]), gd_financiero_rt[cta_des_n2] = "Reserva Técnica Neta", FiltroPeriodoMensualizado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_presupuesto]), gd_financiero_rt[cta_des_n2] = "Reserva Tecnica Neta PyG", FiltroPeriodoMensualizado),
			        "ACUMULADO",    
			            CALCULATE(SUM(gd_financiero_rt[val_presupuesto]), gd_financiero_rt[cta_des_n2] = "Reserva Técnica Neta", FiltroAcumulado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_presupuesto]), gd_financiero_rt[cta_des_n2] = "Reserva Tecnica Neta PyG", FiltroAcumulado),
			        "ANUALIZADO",   
			            CALCULATE(SUM(gd_financiero_rt[val_presupuesto]), gd_financiero_rt[cta_des_n2] = "Reserva Técnica Neta", FiltroAnualizado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_presupuesto]), gd_financiero_rt[cta_des_n2] = "Reserva Tecnica Neta PyG", FiltroAnualizado),
			        0
			    )
			
			-- REAL AÑO ANTERIOR (Y-1)
			VAR REAL_Y_1 = 
			    SWITCH( TipoValor,
			        "MENSUALIZADO", 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"5701","5702"}, FiltroPeriodoAnteriorMensualizado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"4801","4802"}, FiltroPeriodoAnteriorMensualizado),
			        "ACUMULADO",    
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"5701","5702"}, FiltroPeriodoAnteriorAcumulado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"4801","4802"}, FiltroPeriodoAnteriorAcumulado),
			        "ANUALIZADO",   
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"5701","5702"}, FiltroPeriodoAnteriorAnualizado) - 
			            CALCULATE(SUM(gd_financiero_rt[val_saldo_con_signo]), gd_financiero_rt[id_cuenta_contable_n3] IN {"4801","4802"}, FiltroPeriodoAnteriorAnualizado),
			        0
			    )
			
			
			
			RETURN
			SWITCH (
			    SELECTEDVALUE(SelectorCalculo_PG[TipoCalculo]),
			    "Real Act", COALESCE(DIVIDE(REAL_ACT,1000,0),0),
			    "Ppto",COALESCE(DIVIDE(Ppto,1000,0),0),
			    "% Cumpl.",COALESCE((DIVIDE(REAL_ACT,Ppto,0)*100),0),
			    "Real Y-1", COALESCE(DIVIDE(REAL_Y_1,1000,0), 0 ),
			    "%YoY" ,COALESCE((DIVIDE(REAL_ACT-REAL_Y_1,REAL_Y_1,0)*100),0)
			)
			
